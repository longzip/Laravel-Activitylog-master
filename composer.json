{
    "name": "noithatzip/laravel-activitylog",
    "description": "Chuc nang ghi hoat dong cho app noithatzip",
    "keywords": ["noithatzip"],
    "license": "MIT",
    "authors": [{
        "name": "Lo Long",
        "email": "longlv@noithatzip.com",
        "homepage": "https://noithatzip.com"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6",
        "illuminate/database": "^5.6",
        "kalnoy/nestedset": "^4.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "LoLong\\Activitylog\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LoLong\\Activitylog\\ActivitylogServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
